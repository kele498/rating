create table tp_rating
(
    id              int                      not null,
    title       varchar(20) charset utf8 not null,
    cf_name         varchar(20)              null,
    cf_rating       int        default 0     null,
    cf_solve_num    int        default 0     null,
    luogu_id       int                      null,
    luogu_solve_num int                      null,
    is_success      tinyint(1) default 0     null,
    description     varchar(255)             null,
    time            varchar(20)              not null,
    primary key (time, id)
);





